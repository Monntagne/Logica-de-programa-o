programa
{
	
	funcao inicio()
	{
		inteiro n1, n2, soma, multiplicacao, divisao, subtracao
		
		escreva("Digite o primeiro número: ")
		leia (n1)

		
		escreva("Digite o segundo número: ")
		leia (n2)

		soma = n1+n2
		escreva("A soma dos dois é:" , soma,"\n")

		multiplicacao = n1*n2
		escreva("A multiplicação dos dois é:" , multiplicacao,"\n")

		divisao = n1/n2
		escreva("A divisão dos dois é:" , divisao,"\n")

		subtracao = n1-n2
		escreva("A subtração dos dois é:" , subtracao,"\n")

			
	}
}

