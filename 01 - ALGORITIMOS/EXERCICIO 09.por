programa
{
	funcao inicio()
	{
		real fahrenheit, celsius

		escreva("Digite a temperatura em Fahrenheit: ")
		leia(fahrenheit)

		celsius = (fahrenheit - 32.0) * 5.0 / 9.0

		escreva("Temperatura em Celsius: ", celsius, " °C")
	}
}

